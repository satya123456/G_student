                                    <label class=""><strong>&nbsp;&nbsp;&nbsp;&nbsp;MoY&nbsp;&nbsp;</strong></label>
                                }
                                else
                                {
                                    <label class=""><strong>&nbsp;&nbsp;&nbsp;&nbsp;M&Y&nbsp;&nbsp;</strong></label>
                                }
                            </div>&nbsp;&nbsp;
                            <span> : </span>
                            <label><strong>@Model.notes.FirstOrDefault().MONTH</strong>&nbsp;&nbsp;<strong>@Model.notes.FirstOrDefault().year</strong></label>
                        </div>

                    </div>

                    <div class="col-md-2 col-2">
                        <div class="studentImg">
                            <div class="d-flex justify-content-center">
                                <img class="img-fluid" src="https://doeresults.gitam.edu/photo/img.aspx?id=@Model.note.regdno">

                            </div>
                            <div class="signatureImg controllerImg">
                                <img src="https://doeresults.gitam.edu/gitamhallticket/Images/Sumanth-Kumar.png " />
                            </div>
                        </div>
                    </div>
                </div>
            </div>



            <div class="row exam_tbl">

                <div class="col-md-8 col-8">
