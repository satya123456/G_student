                                <label><strong>@Model.notes.FirstOrDefault().psntsem</strong></label>
                            }
                            else
                            {
                                if (Model.notes.FirstOrDefault().college_code.ToUpper() == "GIMSR" && Session["degree_code"].ToString().ToUpper() == "PG")
                                {

                                    if (Model.note.psntsem == "1" || Model.note.psntsem == "I")
                                    {
                                        <label><strong>First</strong></label>
                                    }
                                    else if (Model.note.psntsem == "2" || Model.note.psntsem == "II")
                                    {
