                                        else
                                        {
                                            formattedDate = val.exam_date;
                                        }
                                        <td>@val.subject_code</td>
                                        @if (Session["college_code"].ToString().ToUpper() == "GIMSR" && Session["degree_code"].ToString().ToUpper() == "UG")

                                        {
                                            <td>@val.subject_name</td>
                                        }
                                        else if (Session["college_code"].ToString().ToUpper() != "GIMSR")
                                        {
                                            <td>@val.subject_name</td>
                                        }
                                        @if (Session["college_code"].ToString().ToUpper() == "GIMSR" && Session["degree_code"].ToString().ToUpper() == "PG")
                                        {

                                            <td>@val.papers </td>
                                        }
                                        <td>@formattedDate  (@val.exam_time)</td>
                                        @if (Session["college_code"].ToString().ToUpper() == "GIMSR" && Session["degree_code"].ToString().ToUpper() == "PG")
                                        {

                                            <td>@val.timings  </td>
                                        }
                                    </tr>
                                    k++;
                                    @*}*@
                                }
                                @if (k == 0)
                                {
                                    <tr class="upload_docs">

                                        <td>
                                            <input name="subject_code" maxlength="300" onkeypress="return ValidateSpecialText(event);" class="form-control">
                                        </td>
                                        <td>
                                            <input class="form-control" name="subject_name" maxlength="300" onkeypress="return ValidateSpecialText(event);">
                                        </td>
                                        <td>
                                            <input class="form-control " name="datetime" readonly="readonly" type="text">
                                        </td>



                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div id="qrcode1" class="qrcode1"></div>
                </div>
                @if (Model.studentdetails.Count() > 0)
                {

                    <div class="col-md-5 col-5">
