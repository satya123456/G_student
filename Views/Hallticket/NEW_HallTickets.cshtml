                            <tbody class="el3 upload_item">
                                @{int k = 0; }
                                @foreach (var val in Model.notes)
                                {
                                    string formattedDate = "";
                                    string timings = "";
                                    var datetime = val.exam_date + ((val.exam_time));
                                    <tr class="upload_docs">

                                        @if (DateTime.TryParseExact(val.exam_date, "dd-MM-yyyy", null, System.Globalization.DateTimeStyles.None, out DateTime parsedDate))
