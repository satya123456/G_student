        color: red;
    }

    @@keyframes blinker {
        50% {
            opacity: 0;
        }
    }

    @@media(max-width:400px) {
        .feedback p {
            font-size: 10px;
        }
    }
    #mail table tr{
        cursor:pointer;
    }
    #mail table tr:hover {
        box-shadow: inset 1px 0 0 #dadce0, inset -1px 0 0 #dadce0, 0 1px 2px 0 rgba(60,64,67,.3), 0 1px 3px 1px rgba(60,64,67,.15);
