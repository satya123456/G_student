<script>
    $('#multiSelectproject').on('change', function () {
        debugger;
        var count = $("#project_count").val();
        var remain_count = 4 - count;
        var selectedValues = $('#multiSelectproject').val();
        $('#projectlist').val(selectedValues);
        console.log(selectedValues);
        if (selectedValues.length == remain_count) {

            $('#multiSelectproject').prop('disabled', true);
            $('#multiSelectproject option').each(function () {
                $(this).prop('disabled', true);
            });
        }
    })

    $('.js-example-basic-multiple').on('change', function () {
        debugger;
        var selectedValues = $(this).val();

        if (selectedValues.length == "1") {

            $('#multiSelect2').prop('disabled', true);
            $('#multiSelect2 option').each(function () {
                $(this).prop('disabled', true);
            });
        }
        var selectedValuesJson = JSON.stringify(selectedValues);
        $.ajax({
            type: 'POST',
            url: '/Home/GetProjectsList',
            data: { selectedValues: selectedValuesJson },
            success: function (Result) {
                debugger;
                var dd5 = $("[id*='multiSelectproject']");
                dd5.empty();

                dd5.select2({
                    closeOnSelect: false
                });

                for (var i = 0; i < Result.length; i++) {
                    var newOption = new Option(Result[i].Project_Title, Result[i].id, false, false);
                    dd5.append(newOption).trigger('change');
                }
            },

            error: function (xhr, status, error) {
            }
        });
    });


    function submitproject() {
        debugger;
        var selectedValues = $('#projectlist').val();
        if (selectedValues == "") {
            swal('Please select project', '', 'error');

        } else {
            $.ajax({
                type: 'GET',
                url: './Home/btninsert',
                data: { selectedValues: selectedValues },
                success: function (Result) {
                    debugger;
                    if (Result == "Successfully updated") {
                        swal('Successfully updated', '', 'success');
                        setTimeout(loaddata, 1500);
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        $('#multiSelectproject').prop('disabled', false);
                        $('#multiSelect2').prop('disabled', false);
                        $('#multiSelectproject').empty();
                        $('#multiSelect2').empty();
                        Getselectedsoftwareprojectslist();
                    } else if (Result == "Already selected") {
                        swal('Already selected', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        $('#multiSelectproject').prop('disabled', false);
                        $('#multiSelect2').prop('disabled', false);
                        $('#multiSelectproject').empty();
                        $('#multiSelect2').empty();
                        Getselectedsoftwareprojectslist();
                    }
                    else if (Result == "Max Exceeds") {
                        swal('Max Exceeds', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        $('#multiSelectproject').prop('disabled', false);
                        $('#multiSelect2').prop('disabled', false);
                        $('#multiSelectproject').empty();
                        $('#multiSelect2').empty();
                        Getselectedsoftwareprojectslist();
                    } else {
                        swal('Already selection process completed', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        $('#multiSelectproject').prop('disabled', false);
                        $('#multiSelect2').prop('disabled', false);
                        $('#multiSelectproject').empty();
                        $('#multiSelect2').empty();
                        Getselectedsoftwareprojectslist();
                    }
                    $("#myService").modal("hide");
                    $('body').removeClass('modal-open');
                },
                error: function (result) {

                }
            });
        }

    }
