            data: { 'domain': domain },
            contentType: "application/x-www-form-urlencoded; charset=utf-8",
            dataType: "json",
            success: function (Result) {

                var dd5 = $("[id*='multiSelectproject123']");
                dd5.empty().append('<option selected="selected" value="0">Select</option>');
                for (var i = 0; i < Result.length; i++) {
                    dd5.append($("<option></option>").val(Result[i]["ID"]).html(Result[i]["Project_Title"]));
                }
            }

        });
    }

    function changeproject() {
        debugger;
        var selectedValues = $("#multiSelectproject123 :selected").html();
        var selectedtitleVal = $("#multiSelectproject123 :selected").val();
        var title_id = $('#title_id').val();
        var title = $('#title').val();
        var count = $('#count').val();
        var domain = $('#multiSelect123').val();
        if (selectedValues == "") {
            swal('Please select project', '', 'error');

        }
        else {
            $.ajax({
                type: 'GET',
                url: './Home/btnchangeproject',
                data: { 'selectedValues': selectedValues, 'selectedtitleVal': selectedtitleVal, 'old_title_id': title_id, 'oldtitle': title, 'oldcount': count, 'domain': domain },
                success: function (Result) {
                    debugger;
                    if (Result == "Successfully updated") {
                        swal('Successfully updated', '', 'success');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        Getselectedsoftwareprojectslist();
                    } else if (Result == "Already selected") {
                        swal('Already selected', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        Getselectedsoftwareprojectslist();
                    }
                    else if (Result == "Max Exceeds") {
                        swal('Max Exceeds', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        Getselectedsoftwareprojectslist();
                    } else {
                        swal('Already selection process completed', '', 'error');
                        $('#tabfill3, .tabfill3').addClass('active show');
                        $('#tabfill1, .tabfill1').removeClass('active show');
                        Getselectedsoftwareprojectslist();
                    }
                    $("#myService123").modal("hide");
                    $('body').removeClass('modal-open');
                },
                error: function (result) {

                }
            });
        }

    }
</script>